<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns="http://camel.apache.org/schema/spring">
	<route id="I0020">
		<from uri="file:/usr/surce?move=/usr/archieve/${file:name}" />
		<to uri="audit:FileInlet?level=info" />
		<marshal ref="baseWith64">base64</marshal>
		<setHeader headerName="CamelHttpMethod">
			<constant>GET</constant>
		</setHeader>
		<setHeader headerName="CamelHttpQuery">
			<constant>authMethod=Basic&amp;authUsername=user123&amp;authPassword=pass123</constant>
		</setHeader>
		<to uri="netty-http:https://example.com/api?a=test"/>
		<transform>
			<simple>${bodyAs(String)}</simple>
		</transform>
		<to uri="audit:Rest?level=INFO"/>
		<to uri="file:/usr/target/FT.txt" />
		<to uri="audit:FileOutlet?level=info" />
	</route>
</routes>
